{
  "name": "Unmock API tools",
  "version": "0.0.1",
  "description": "Easily create OpenAPI specification from written API documentation",
  "short_name": "Unmock",
  "homepage_url": "https://unmock.io",
  "permissions": [
    "activeTab",
    "contextMenus",
    "declarativeContent",
    "downloads",
    "storage",
    "tabs"
  ],
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "all_frames": false,
      "run_at": "document_idle",
      "js": ["contentScript.bundle.js"]
    }
  ],
  "browser_action": {
    "default_icon": {
      "48": "icon48.png"
    },
    "default_title": "Label APIs",
    "default_popup": "popup.html"
  },
  "commands": {
    "toggle-unmock": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y",
        "mac": "Command+Shift+Y"
      },
      "description": "Toggle unmock"
    }
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
